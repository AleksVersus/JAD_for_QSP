# #sqrt#
$args[0] = $args[0]
if args[1] = 0: args[1] = 100
if $args[2]='': $args[2]='1'
:still
$args['res']=$func('#:#',$func('#+#',$args[2],$func('#:#',$args[0],$args[2],args[1])),'2',args[1])
$args['quad'] = $func('#*#',$args['res'],$args['res'])
if $args['res']!$args[2]:
	$args[2]=$args['res']
	jump 'still'
else
	$result=$args['res']
	exit
end
