! добавление отличительных свойств на предмет
if $cmd_ = '' or instr($cmd_,'get.obj')<>0:
	if $new_item_<>'<obj> ' and $new_item_<>'':
		!@ добавляем исходные позициии типа
		local $props_list_ = @em.tag.getCont($new_item_,'np')	&	!@	получаем набор признаков
		$res_ = $new_item_	&	!@	запоминаем тело предмета
		!@ если дальность удара по функции получается и не присутствует в запомненом теле, добавляем дальность удара
		local du_ = @get.obj.DU($props_list_)
		if  du_ <> 0 and instr($res_,'[DU:') = 0: $new_item_ += ' [DU:<<du_>>]'
		!@ если позиции для предмета есть, добавляем.
		local $positions_ = $func('get.obj.pos',$props_list_,$res_)
		if $positions_ <> '': $new_item_ += ' [pos: <<$positions_>> :pos]'
		!@ Слияние свойств. Приоритет у $req_item_
		$output_obj_ = @remastering.obj($req_item_,$new_item_)
		local $OID_start_ = $iif(instr($output_obj_,'[oneself]')<>0, $UCASE($obj_kind_), $obj_kind_)
		if instr($output_obj_,'[maxchrg:')<>0 and charge_ = 0:
			charge_ = func('em.tag.getNum',$output_obj_,'charge')
		elseif charge_ <> 0 and  instr($output_obj_,'[maxchrg:')!0:
			charge_ = charge_
		else
			charge_ = 0
		end
		local $output_code_ = $iif($run_array_<>'', $run_array_, '')
		if instr($cmd_,'get.obj')=0:
			if $place_='': $place_ = @get.loc.id() 
			$result = @add.new.obj($output_obj_, $place_, $OID_start_, number_, charge_, $output_code_)
		elseif instr($cmd_,'get.run_array')!0:
			$result = $output_code_
		else
			$result = $output_obj_
		end
		exit
	else
		msg 'Внимание!!! Ошибка базы "base.new.obj". Не найден объект "<<$obj_kind_>>". Проверьте правильность вызова объекта.'
		exit
	end
elseif $cmd_='text':
	if  $res_text_='':
		$result='-описание не создано-'
	else
		$result=$res_text_
	end
	exit
elseif $cmd_='image':
	if $res_image_='':
		$result=@base.img('onim')
		exit
	else:
		$result=$res_image_
		exit
	end
elseif $cmd_='book':
	if $res_book_='':
		$result='-не удаётся прочесть-'
		exit
	else
		$result=$res_book_
		exit
	end
elseif $cmd_='unmount':
	local $unmount_ = @remastering.tag.number($req_item_, $res_ingredients_)
	if $unmount_ <> '':
		$result = $unmount_
		exit
	else
		$result = 'cant unmount'
		exit
	end
end
