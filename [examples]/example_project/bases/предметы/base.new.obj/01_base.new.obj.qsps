!@ ------------------------------ первая часть генератора. не подлежит редактированию -----------------
!@ принятые аргументы
local $req_item_, $cmd_, $place_ = $args[0], $args[1], $args[2]
local number_, charge_, $code_ = args[3], args[4], $args[5]
!@ другие локальные переменные
local $obj_kind_, $run_array_, $new_item_, $random_
local $res_text_, $res_ingredients_, $res_book_, $res_image_, $res_
if number_ = 0: number_ = 1
if $cmd_ = '' or instr($cmd_,'get.obj')<>0: $new_item_+='<obj> ' else $new_item_+=''
$obj_kind_ = $replace($replace($strfind($req_item_,'\[:[\S]+:\]'),'[:'),':]')
$run_array_ += $code_
! ------------------------------ первая часть генератора. не подлежит редактированию ----------------
!@ выбор лута по признаку и т.п.
!@ :случайный_лут
!@ $args['jump']=$func('base.obj.new.get',$args['jump'])
!@ cjump $args['jump']
