	$ok["/ла"]=''
	$ok["/а"]=''
	$ok["ый/ая"]='ый'
	$ok["ой/ая"]='ой'
	$ok["ым/ой"]='ым'
	$ok["ий/ая"]='ий'
	$ok["ен/на"]="ен"
	$ok["ком/цей"]='ком'
	$ok["сам/сама"]='сам'
	$ok["один/одна"]='один'
