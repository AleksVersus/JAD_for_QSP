# hero.count
! счётчики 
$args['hero.id']=$func('get.id.obj','<hero>')
! подсчитывающий очки опыта
$cur['count']=$func('add.new.obj','<counter> [:hero.level:] [name:Опыт:name] [np:[level]:np] [l:уровень|уровня|уровню|уровень|уровнем|уровне:l] [m:|:m][hide]',$args['hero.id'],'COUNTER')
! на экране должно выводиться примерно следующее:
! Орк: 1 уровень
! Опыт: 100|200
point_count['hero.level.level'] = 0
point_count['hero.level.point'] = 0
point_count['hero.level.marker']= 100

gs 'add.new.obj','<counter> [:hero.teaching:] [name:Очки обучения:name] [np:[easy]:np] [up: +:up] [hide]',$args['hero.id'],'COUNTER'
point_count['hero.teaching.point']=0
point_count['hero.teaching.up']=5	&	!	значение по умолчанию
