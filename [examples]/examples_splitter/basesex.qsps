# maxVar
local max_, cur_
$result = $args[0]
loop local i = 0 while $args[i] <> '' step i += 1:
	cur_ = arritem($args[i], 0)
	if cur_ > max_:
		$result = $args[i]
		max_ = cur_
	end
end
